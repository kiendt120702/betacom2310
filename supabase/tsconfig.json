{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["deno.ns", "deno.window"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["@supabase/functions-js/edge-runtime"]
  },
  "include": ["**/*.ts"]
}